    <ImageView
        android:id="@+id/my_bear_rightleg"
        android:layout_width="30dp"
        android:layout_height="60dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/bear_rightleg_imageview"
        app:layout_constraintEnd_toEndOf="@+id/my_bear_body"
        app:layout_constraintTop_toTopOf="@+id/bear_rightleg_imageview"
